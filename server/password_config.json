{
    "settings":{
        "minimum-length" : 8,
        "must-contain-numbers" : true,
        "must-contain-lowercase" : true,
        "must-contain-uppercase" : true,
        "must-contain-special-character" : true
    },

    "regex" : {
        "must-contain-numbers" : "[0-9]",
        "must-contain-lowercase" : "[a-z]",
        "must-contain-uppercase" : "[A-Z]",
        "must-contain-special-character" : "[$@$!%*#?&]"
    }
}